generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id
  name  String?
  email String  @unique(map: "sqlite_autoindex_User_2")
}

model erp {
  id        String @id @map("_id")
  cpName    String
  weight    Float
  area      String
  date      String
  est_price Float?
  trd       trd?
}

model trd {
  id     String @id @map("_id")
  cpName String
  weight Float
  area   String
  date   String
  price  Float
  erp    erp    @relation(fields: [id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}
